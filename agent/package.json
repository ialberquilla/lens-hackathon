    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "zksync-ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
